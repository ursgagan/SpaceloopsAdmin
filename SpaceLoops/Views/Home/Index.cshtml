@{
    Layout = "_Layout";
}


<section class=" slider_section ">
    <div id="carouselExampleIndicators" class="carousel slide" data-ride="carousel">
        <div class="carousel-inner">
            <div class="carousel-item active">
                <div class="container">
                    <div class="row">
                        <div class="col-md-7">
                            <div class="detail-box">
                                <h1>
                                    Top Website & Mobile App <br>
                                    <span>
                                        Design and Development Company
                                    </span>
                                </h1>
                                <p>
                                    Turn Your Ideas Into Reality with Top-Notch Web and App Development Company
                                </p>
                                <div class="btn-box">
                                    <a href="/Home/ContactUs" class="btn-2"> Contact Us </a>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="carousel-item ">
                <div class="container">
                    <div class="row">
                        <div class="col-md-7">
                            <div class="detail-box">
                                <h1>
                                    Design. Develop. <br>
                                    <span>
                                        Deliver.
                                    </span>
                                </h1>
                                <p>
                                    Our approach towards work is fast-paced, yet a detailed delve into
                                    brand objectives. Besides, we like to keep things agile and flexible to
                                    design high-quality, scalable solutions for forward-thinking brands
                                </p>
                                <div class="btn-box">
                                    <a href="/Home/ContactUs" class="btn-2"> Contact Us </a>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="carousel-item ">
                <div class="container">
                    <div class="row">
                        <div class="col-md-7">
                            <div class="detail-box">
                                <h1>
                                    We Make   <br>
                                    <span>
                                        IT Happen
                                    </span>
                                </h1>
                                <p>
                                    Get ready to transform your business ideas into reality!
                                </p>
                                <div class="btn-box">
                                    <a href="/Home/ContactUs" class="btn-2"> Contact Us </a>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="container idicator_container">
            <ol class="carousel-indicators">
                <li data-target="#carouselExampleIndicators" data-slide-to="0" class="active"></li>
                <li data-target="#carouselExampleIndicators" data-slide-to="1"></li>
                <li data-target="#carouselExampleIndicators" data-slide-to="2"></li>
            </ol>
        </div>
    </div>
</section>
<!-- end slider section -->
    </div>

<!-- about section -->

<section class="about_section layout_padding">
    <div class="container">
        <div class="row">
            <div class="col-md-6 px-0">
                <div class="img_container">
                    <div class="img-box">
                        <img src="~/spaceloopstemplate/images/about-img.jpg" alt="" />
                    </div>
                </div>
            </div>
            <div class="col-md-6 px-0">
                <div class="detail-box">
                    <div class="heading_container ">
                        <h2>
                            Who Are We?
                        </h2>
                    </div>
                    <p>
                        We are diverse, passionate, and highly curious tech enthusiasts who are sowing the seeds for a better tomorrow. We collaborate with businesses, enterprises, organizations, and government agencies to streamline their operations. The faster rolling and growing numbers themselves tell why we are a leader in IT solution providers.
                    </p>
                    <div class="btn-box">
                        <a href="/Home/About" class="btn-1"> About Us </a>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>

<!-- end about section -->
<!-- service section -->

<section class="service_section layout_padding" id="ServicesDiv">
    <div class="container">
        <div class="heading_container heading_center">
            <h2>
                Our services
            </h2>
        </div>
        <div class="row">
            <div class="col-md-4">
                <div class="box ">
                    <div class="img-box">
                        <img width="70" height="70" src="/AdminTemplate/ServiceImages/icons8-web-development-50.png" alt="imac-settings" />
                    </div>
                    <div class="detail-box">
                        <h6>
                            Web Development
                        </h6>
                        <p>
                            SpaceLoops's highly efficient team members utilize cross-domain knowledge and possess vast knowledge of .NET, Angular, ReactJs, Javascript and various open-source platforms.
                        </p>
                        <div class="btn-box">
                            <a href="/Home/ContactUs" class="btn-1"> Contact Us </a>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-md-4">
                <div class="box ">
                    <div class="img-box">
                        <img width="70" height="70" src="/AdminTemplate/ServiceImages/icons8-web-design-64.png" alt="imac-settings" />
                    </div>
                    <div class="detail-box">
                        <h6>
                            Product Design
                        </h6>
                        <p>
                            Our designers are very keen and enthusiastic about providing top-notch services that can truly reflect your business and connect with your audience.
                        </p>
                        <div class="btn-box">
                            <a href="/Home/ContactUs" class="btn-1"> Contact Us </a>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-md-4">
                <div class="box " style="height:373px">
                    <div class="img-box">
                        <img width="70" height="70" src="/AdminTemplate/ServiceImages/icons8-it-48.png" alt="imac-settings" />
                    </div>
                    <div class="detail-box">
                        <h6>
                            IT Consulting
                        </h6>
                        <p>
                            Transform your technology landscape with the help of IT Consulting Services, your go-to experts for digital solutions. Let's Discuss your IT Consulting needs With our Experts!
                        </p>
                        <div class="btn-box">
                            <a href="/Home/ContactUs" class="btn-1"> Contact Us </a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>

<section class="client_section layout_padding">
    <div class="container ">
        <div class="heading_container heading_center">
            <h2>
                What our Client Says
            </h2>
        </div>
        <div id="carouselExampleControls" class="carousel slide" data-ride="carousel">
            <div class="carousel-inner">
                <div class="carousel-item active">
                    <div class="box">
                        <div class="img-box">
                            <img src="~/spaceloopstemplate/images/client.png" alt="">
                        </div>
                        <div class="detail-box">
                            <h4>
                                Gurdeep Singh
                            </h4>
                            <p>
                                I recently had the opportunity to work with SpaceLoops for IT Consulting Services need and was extremely impressed with the results.
                            </p>
                        </div>
                    </div>
                </div>
                <div class="carousel-item ">
                    <div class="box">
                        <div class="img-box">
                            <img src="~/spaceloopstemplate/images/client.png" alt="">
                        </div>
                        <div class="detail-box">
                            <h4>
                                Harinder Singh
                            </h4>
                            <p>
                                We highly recommend SpaceLoops for Web Development services to any organization looking to optimize their technology infrastructure and achieve their technology goals.
                            </p>
                        </div>
                    </div>
                </div>
            </div>
            <div class="carousel_btn-box">
                <a class="carousel-control-prev" href="#carouselExampleControls" role="button" data-slide="prev">
                    <i class="fa fa-angle-left" aria-hidden="true"></i>
                    <span class="sr-only">Previous</span>
                </a>
                <a class="carousel-control-next" href="#carouselExampleControls" role="button" data-slide="next">
                    <i class="fa fa-angle-right" aria-hidden="true"></i>
                    <span class="sr-only">Next</span>
                </a>
            </div>
        </div>
    </div>
</section>

